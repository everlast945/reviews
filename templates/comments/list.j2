{% extends "base.j2" %}

{% block content %}
<table>
    <thead>
        <tr>
            <th>Фамилия</th>
            <th>Имя</th>
            <th>Отчество</th>
            <th>Телефон</th>
            <th>Почта</th>
            <th>Комментарий</th>
        </tr>
    </thead>
    <tbody>
        {% for comment in comments %}
            <tr>
                <td>{{comment.1 }}</td>
                <td>{{comment.0 }}</td>
                <td>{{comment.2 }}</td>
                <td>{{comment.3 }}</td>
                <td>{{comment.4 }}</td>
                <td>{{comment.5 }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>



{% endblock %}